<%- include(viewPath('home/layout/header')) %>
<section class="seo-page w100 grid-y">
  <% if(typeof aboutPage.headerImageUrl !== 'undefined' ) { %>
  <div class="header-slider w100" style="background-image: url('<%= aboutPage.headerImageUrl.destination %>')">
  <% } else { %>
  <div class="header-slider w100">
  <% }%>  
    <div class="overlay grid align-center-middle">
      <div class="container">
        <h2><%= aboutPage.headerTitle %></h2>
      </div>
    </div>
  </div>
  <div class="seo-desc wrapper grid align-center">
    <div class="container">
      <div class="wrap w100 grid-x">
        <div class="decription pl-5 grid-y">
          <%- aboutPage.desc1Desc %>
        </div>
        <div class="image">
          <img src="<%= aboutPage.desc1ImageUrl.destination %>" alt="SEO">
